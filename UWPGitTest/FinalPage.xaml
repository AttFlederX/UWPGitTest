        <TextBox HorizontalAlignment="Left" Margin="1160,115,0,0" Text="TextBox" VerticalAlignment="Top" Width="280"/>
        <SplitView HorizontalAlignment="Left" Height="100" Margin="715,645,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5">
